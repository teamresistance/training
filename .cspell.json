{
  "language": "en,en-US",
  "version": "0.2",
  "checkFileNames": false,
  "ignorePaths": [
    "**/.cspell.json",
    "**/.gitignore",
    "**/.git/**",
    ".github/**/*.yml",
    ".github/CODEOWNERS",
    "hugo.toml"
  ],
  "ignoreRegExpList": [
    "/`+([\\s\\S]*?)`+/g",
    "/^```[\\s\\S]*?^```/gm",
    "/\\w*\\_\\w*/g"
  ],
  "words": [
    "AprilTag",
    "AprilTags",
    "CTRE",
    "deadband",
    "feedforward",
    "holonomic",
    "Kalman",
    "pathfinding",
    "pathing",
    "pathplanner",
    "pathplanning",
    "teleop",
    "teleoperated",
    "timestamping"
  ]
}
